    {
        "id": "81b56389e27951b0",
        "type": "function",
        "z": "728ca729fe6670e7",
        "name": "Formatear Datos",
        "func": "if (msg.topic.includes(\"eje/0\")) msg.payload = \"LX:\" + msg.payload;\nelse if (msg.topic.includes(\"eje/1\")) msg.payload = \"LY:\" + msg.payload;\nelse if (msg.topic.includes(\"eje/2\")) msg.payload = \"RX:\" + msg.payload;\nelse if (msg.topic.includes(\"eje/3\")) msg.payload = \"RY:\" + msg.payload;\nelse if (msg.topic.includes(\"eje/4\")) msg.payload = \"TL:\" + msg.payload;\nelse if (msg.topic.includes(\"eje/5\")) msg.payload = \"TR:\" + msg.payload;\nelse if (msg.topic.includes(\"eje/6\")) msg.payload = \"DX:\" + msg.payload;\nelse if (msg.topic.includes(\"eje/7\")) msg.payload = \"DY:\" + msg.payload;\nelse if (msg.topic.includes(\"boton\")) {\n    var parts = msg.topic.split(\"/\");\n    var btn = parts[parts.length - 1];\n    msg.payload = \"BTN\" + btn + \":\" + msg.payload;\n}\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 310,
        "y": 420,
        "wires": [
            [
                "6657c717deb20039"
            ]
        ]
    },
    {
        "id": "c1eac3f483d7929c",
        "type": "ui_template",
        "z": "728ca729fe6670e7",
        "group": "01df994d1db6567c",
        "name": "Visualizador Joy Izquierdo",
        "order": 1,
        "width": 6,
        "height": 6,
        "format": "<script>\n(function(scope) {\n    scope.$watch('msg', function(msg) {\n        if (!scope.joyState) scope.joyState = { x: 0, y: 0 };\n        if (msg) {\n            if (msg.topic.indexOf('eje/0') !== -1) scope.joyState.x = parseInt(msg.payload) / 10;\n            if (msg.topic.indexOf('eje/1') !== -1) scope.joyState.y = parseInt(msg.payload) / 10;\n        }\n    });\n})(scope);\n</script>\n<style>\n    .joy-container { display: flex; flex-direction: column; align-items: center; justify-content: center; height: 100%; }\n</style>\n<div class=\"joy-container\">\n    <h4 style=\"margin-bottom: 10px;\">Joystick Izquierdo</h4>\n    <svg width=\"200\" height=\"200\" style=\"border: 2px solid #555; background: #111; border-radius: 50%;\">\n        <line x1=\"100\" y1=\"0\" x2=\"100\" y2=\"200\" stroke=\"#333\" />\n        <line x1=\"0\" y1=\"100\" x2=\"200\" y2=\"100\" stroke=\"#333\" />\n        <circle \n            ng-attr-cx=\"{{ (joyState.x || 0) + 100 }}\" \n            ng-attr-cy=\"{{ (joyState.y || 0) + 100 }}\" \n            r=\"12\" fill=\"#39ff14\" stroke=\"white\" stroke-width=\"2\" />\n    </svg>\n</div>",
        "storeOutMessages": true,
        "fwdInMessages": true,
        "resendOnRefresh": true,
        "templateScope": "local",
        "className": "",
        "x": 550,
        "y": 180,
        "wires": [
            []
        ]
    },

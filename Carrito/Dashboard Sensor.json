        "name": "Visualizador Joy Derecho",
        "order": 1,
        "width": 6,
        "height": 6,
        "format": "<script>\n(function(scope) {\n    scope.$watch('msg', function(msg) {\n        if (!scope.joyStateR) scope.joyStateR = { x: 0, y: 0 };\n        if (msg) {\n            if (msg.topic.indexOf('eje/2') !== -1) scope.joyStateR.x = parseInt(msg.payload) / 10;\n            if (msg.topic.indexOf('eje/3') !== -1) scope.joyStateR.y = parseInt(msg.payload) / 10;\n        }\n    });\n})(scope);\n</script>\n<style>\n    .joy-container { display: flex; flex-direction: column; align-items: center; justify-content: center; height: 100%; }\n</style>\n<div class=\"joy-container\">\n    <h4 style=\"margin-bottom: 10px;\">Joystick Derecho</h4>\n    <svg width=\"200\" height=\"200\" style=\"border: 2px solid #555; background: #111; border-radius: 50%;\">\n        <line x1=\"100\" y1=\"0\" x2=\"100\" y2=\"200\" stroke=\"#333\" />\n        <line x1=\"0\" y1=\"100\" x2=\"200\" y2=\"100\" stroke=\"#333\" />\n        <circle \n            ng-attr-cx=\"{{ (joyStateR.x || 0) + 100 }}\" \n            ng-attr-cy=\"{{ (joyStateR.y || 0) + 100 }}\" \n            r=\"12\" fill=\"#0099ff\" stroke=\"white\" stroke-width=\"2\" />\n    </svg>\n</div>",

    {
        "id": "5e556b6d333dbd18",
        "type": "ui_template",
        "z": "728ca729fe6670e7",
        "group": "800c05cac4f3192e",
        "name": "Visualizador Cruceta",
        "order": 2,
        "width": "5",
        "height": "5",
        "format": "<script>\n(function(scope) {\n    scope.$watch('msg', function(msg) {\n        if (!scope.hatState) scope.hatState = { x: 0, y: 0 };\n        if (msg) {\n            var parts = msg.payload.toString().split(',');\n            if (parts.length === 2) {\n                scope.hatState.x = parseInt(parts[0]);\n                scope.hatState.y = parseInt(parts[1]);\n            }\n        }\n    });\n})(scope);\n</script>\n<style>\n    .dpad-container { display: flex; flex-direction: column; align-items: center; justify-content: center; height: 100%; }\n    .dpad-row { display: flex; }\n    .dpad-btn { \n        width: 40px; height: 40px; margin: 2px; \n        background: #333; border: 1px solid #555; border-radius: 4px;\n        display: flex; align-items: center; justify-content: center;\n        color: #888; font-weight: bold;\n    }\n    .dpad-center { width: 40px; height: 40px; margin: 2px; background: transparent; }\n    .dpad-btn.active { background: #ffa500; color: #000; box-shadow: 0 0 10px #ffa500; border-color: #ffcc00; }\n</style>\n<div class=\"dpad-container\">\n    <div class=\"dpad-row\">\n        <div class=\"dpad-btn\" ng-class=\"{'active': hatState.y === 1}\">▲</div>\n    </div>\n    <div class=\"dpad-row\">\n        <div class=\"dpad-btn\" ng-class=\"{'active': hatState.x === -1}\">◀</div>\n        <div class=\"dpad-center\"></div>\n        <div class=\"dpad-btn\" ng-class=\"{'active': hatState.x === 1}\">▶</div>\n    </div>\n    <div class=\"dpad-row\">\n        <div class=\"dpad-btn\" ng-class=\"{'active': hatState.y === -1}\">▼</div>\n    </div>\n</div>",
        "storeOutMessages": true,
        "fwdInMessages": true,
        "resendOnRefresh": true,
        "templateScope": "local",
        "className": "",
        "x": 760,
        "y": 500,
        "wires": [
            []
        ]
    },
